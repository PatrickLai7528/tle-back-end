{
  "name": "tle-back-end",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-tle-back-end",
    "stop": "egg-scripts stop --title=egg-server-tle-back-end",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "crypto": "^1.0.1",
    "egg": "^2.6.1",
    "egg-cors": "^2.2.3",
    "egg-mongoose": "^3.2.0",
    "egg-onerror": "^2.1.0",
    "egg-passport": "^2.1.0",
    "egg-passport-jwt": "^1.0.0",
    "egg-scripts": "^2.6.0",
    "http-status-codes": "^1.4.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/axios": "^0.14.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/mocha": "^2.2.40",
    "@types/mongoose": "^5.7.12",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "@types/uuid": "^7.0.2",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "eslint": "^6.7.2",
    "eslint-config-egg": "^8.0.0",
    "husky": "^4.2.5",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.1",
    "tslib": "^1.9.0",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "Patrick Lai",
  "license": "MIT"
}
